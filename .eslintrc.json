{
    "env": {
        "browser": true,
        "es2021": true,
        "jquery": true
    },
    "extends": [
        "plugin:fsd/all",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": "off"
    },
    "plugins": ["fsd"],
    "settings": {
        "import/resolver": "webpack"
    }
}
